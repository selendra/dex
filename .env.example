# Private key for deployment (DO NOT commit real keys!)
PRIVATE_KEY=your_private_key_here

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key

# Gas reporting
REPORT_GAS=true

# Selendra Network Configuration
SELENDRA_RPC_URL=https://rpc.selendra.org
SELENDRA_CHAIN_ID=1961

# Selendra Token Addresses
SELENDRA_TUSD_ADDRESS=your_tusd_address
SELENDRA_TBROWN_ADDRESS=your_tbrown_address
SELENDRA_TSMART_ADDRESS=your_tsmart_address
SELENDRA_TZANDO_ADDRESS=your_tzando_address

# Selendra DEX Contract Addresses
SELENDRA_POOL_MANAGER_ADDRESS=your_pool_manager_address
SELENDRA_STATE_VIEW_ADDRESS=your_state_view_address
SELENDRA_LIQUIDITY_MANAGER_ADDRESS=your_liquidity_manager_address
SELENDRA_SWAP_ROUTER_ADDRESS=your_swap_router_address
